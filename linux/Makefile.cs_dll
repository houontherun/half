#CFLAGS+= -g -ggdb

INCLUDEDIRS= -I$(GAME_SHARED_SRC_DIR) -I$(PUBLIC_SRC_DIR) -I$(COMMON_SRC_DIR) -I$(GAME_SRC_DIR) -I$(CSTRIKE_SRC_DIR) -I$(ENGINE_SRC_DIR) -I$(HL2_SRC_DIR) -I$(GAME_SHARED_HL2_SRC_DIR) -I$(GAME_SHARED_CSTRIKE_SRC_DIR)
